	    <attribute>
	    	<name>r2</name>
	    	<required>false</required>
	    	<rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	    	<name>r3</name>
	    	<required>false</required>
	    	<rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	    	<name>r4</name>
	    	<required>false</required>
	    	<rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	    	<name>r5</name>
	    	<required>false</required>
	    	<rtexprvalue>true</rtexprvalue>
	    </attribute>
	</tag>
	
	<tag>
	    <name>commas</name>
	    <tag-class>com.interpark.book.commons.customtags.CommasTag</tag-class>
	    <body-content>empty</body-content>
	    <attribute>
	    	<name>val</name>
	    	<required>true</required>
	    	<rtexprvalue>true</rtexprvalue>
	    </attribute>
    </tag>
    
    <tag>
	    <name>script</name>
	    <tag-class>com.interpark.book.commons.customtags.ScriptTag</tag-class>
	    <body-content>empty</body-content>
	    <attribute>
	    	<name>src</name>
	    	<required>true</required>
	    	<rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <dynamic-attributes>true</dynamic-attributes>
    </tag>
    
    <tag>
	    <name>link</name>
	    <tag-class>com.interpark.book.commons.customtags.LinkTag</tag-class>
