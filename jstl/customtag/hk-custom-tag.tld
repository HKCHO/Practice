	    </attribute>
	    <attribute>
	    	<name>addSecond</name>
	    	<required>false</required>
	    	<rtexprvalue>true</rtexprvalue>
	    </attribute>
	    <attribute>
	    	<name>default</name>
	    	<required>false</required>
	    	<rtexprvalue>true</rtexprvalue>
	    </attribute>
	</tag>
	<tag>
	    <name>type</name>
	    <tag-class>com.interpark.book.commons.customtags.TypeTag</tag-class>
