<?xml version="1.0" encoding="UTF-8"?>
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>2.3</jsp-version>
	<short-name>hk</short-name>
	<info>현고니 태그</info>
	
  	<function>
        <description>쉼표형으로 숫자를 표시합니다.</description>
        <name>commas</name>
        <function-class>com.interpark.book.commons.customtags.CustomFunction</function-class>
        <function-signature>String commas( java.lang.String )</function-signature>
	</function>
	<function>
        <description>Map 객체를 JsonObject 객체로 변환합니다.</description>
        <name>toJson</name>
        <function-class>com.interpark.book.commons.customtags.CustomFunction</function-class>
        <function-signature>String toJson( java.util.Map )</function-signature>
	</function>
	<function>
        <description>전화번호 길이에 따라 분리 단위를 나누고 지정한 위치(idx)에 해당하는 전화번호 단위를 가져옵니다.</description>
        <name>splitAsTel</name>
        <function-class>com.interpark.book.commons.customtags.CustomFunction</function-class>
        <function-signature>String splitAsTel( java.lang.String, java.lang.Integer )</function-signature>
	</function>
	
	<function>
        <description>HTML태그를 제거합니다.</description>
        <name>htmlRemove</name>
        <function-class>com.interpark.book.commons.customtags.CustomFunction</function-class>
        <function-signature>String htmlRemove( java.lang.String )</function-signature>
	</function>
	
	<function>
        <description>enter를 <br />로 치환합니다.</description>
        <name>enterToBr</name>
        <function-class>com.interpark.book.commons.customtags.CustomFunction</function-class>
        <function-signature>String enterToBr( java.lang.String )</function-signature>
	</function>
	
	<tag>
	    <name>date</name>
